@using BlazorDemo.Models

<div class="photo-container card">
    <img class="card-img-top" src="@photo.Path" />
    <div class="card-body">
        <span>@photo.DateTaken</span>
    </div>
</div>

@code {
    [Parameter]
    public Photo photo { get; set; }

    [Parameter]
    public bool Selected { get; set; }
}